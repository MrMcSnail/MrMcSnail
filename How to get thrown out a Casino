let count = 0;

function cc(card) {
  // Only change code below this line
  switch (card) {
    //the case in which the count increases and the pack has more high cards
    case 2:
    case 3:
    case 4:
    case 5:
    case 6:
      count++
      break;
    //the case in which the count stays the same the pack has the same odds
    case 7:
    case 8:
    case 9:
      count = count
      break;
    //the case in which the count decreases and the the pack has less high cards
    case 10:
    case 'J':
    case 'Q':
    case 'K':
    case 'A':
      count--
  }

  if(count)
  return "Change Me";
  // Only change code above this line
}

cc(2); cc(3); cc(7); cc('K'); cc('A');
